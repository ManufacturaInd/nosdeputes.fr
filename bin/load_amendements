
echo Parse Amendements
echo > /tmp/load_amendements.log
cd batch/amendements
# rm -f html/*
# bash download_amendements.sh >> /tmp/load_amendements.log
# rm -f json/*
# bash parse_amendements.sh >> /tmp/load_amendements.log
cd -
php symfony cc
for file in $(ls batch/amendements/json)
do
    echo $file
    cp batch/amendements/json/$file batch/amendements/input/
    php symfony load:Amdmts >> /tmp/load_amendements.log
    php symfony cc
done
