#!/bin/bash

echo Parse Amendements
echo > /tmp/load_amendements.log
cd batch/amendements
# rm -f xml/*
# perl parse_amendements.pl >> /tmp/load_amendements.log
cd -
php symfony cc
for file in $(ls batch/amendements/xml)
do
    echo $file
    cp batch/amendements/xml/$file batch/amendements/input/
    php symfony load:Amdmts >> /tmp/load_amendements.log
    php symfony cc
done
